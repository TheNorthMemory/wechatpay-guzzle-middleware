{
    "name": "wechatpay/wechatpay-guzzle-middleware",
    "version": "1.0.0",
    "description": "WechatPay API V3 Guzzle Middleware",
    "type": "library",
    "keywords": [
        "wechatpay", "chainable requests client", "aes-gcm", "rsa-oaep"
    ],
    "authors": [
        {
            "name": "WeChatPay Community",
            "homepage": "https://developers.weixin.qq.com/community/pay"
        }
    ],
    "homepage": "https://wechatpay-api.gitbook.io/wechatpay-api-v3/",
    "license": "Apache-2.0",
    "require": {
        "guzzlehttp/uri-template": "*",
        "guzzlehttp/guzzle": ">=7.0",
        "php": ">=7.2",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": { "WechatPay\\GuzzleMiddleware\\" : "src/" }
    },
    "bin": [
        "bin/CertificateDownloader.php"
    ],
    "suggest": {
        "ext-bcmath": "Require bcmath in php 5.* version.",
        "guzzlehttp/uri-template": "For using wechatpay guzzle url-template(RFC6570) feature.",
        "guzzlehttp/guzzle": "For using wechatpay guzzle middleware."
    }
}
